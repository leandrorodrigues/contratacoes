<div class="page-header">
  <%= link_to contract_events_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contract_event_path(@contract_event), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contract_event</h1>
</div>

<dl class="dl-horizontal">
  <dt>Contract:</dt>
  <dd><%= @contract_event.contract %></dd>

  <dt>Title:</dt>
  <dd><%= @contract_event.title %></dd>

  <dt>Modified at:</dt>
  <dd><%= @contract_event.modified_at %></dd>

</dl>
