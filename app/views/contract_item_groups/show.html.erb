<div class="page-header">
  <%= link_to contract_item_groups_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contract_item_group_path(@contract_item_group), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contract_item_group</h1>
</div>

<dl class="dl-horizontal">
  <dt>Contract:</dt>
  <dd><%= @contract_item_group.contract %></dd>

  <dt>Title:</dt>
  <dd><%= @contract_item_group.title %></dd>

  <dt>Number:</dt>
  <dd><%= @contract_item_group.number %></dd>

  <dt>Contracts:</dt>
  <dd><%= @contract_item_group.contracts %></dd>

</dl>
